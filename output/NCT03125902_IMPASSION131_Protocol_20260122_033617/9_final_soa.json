            "notes": [
              {
                "id": "soa_fn_1",
                "text": "[a1] If a scheduled treatment visit cannot be completed due to a holiday, dosing may be postponed to the earliest next date.",
                "instanceType": "CommentAnnotation"
              },
              {
                "id": "soa_fn_2",
                "text": "[a2] Assessments scheduled on the day of study treatment administration (Day 1) of each cycle should be performed prior to study treatment infusion.",
                "instanceType": "CommentAnnotation"
              },
              {
                "id": "soa_fn_3",
                "text": "[a3] Paclitaxel will be administered at the 90 mg/m2 dose via 1-hour IV infusion on Days 1, 8, and 15 of every 28-day cycle.",
                "instanceType": "CommentAnnotation"
              },
              {
                "id": "soa_fn_4",
                "text": "[b] Patients will be asked to return to the clinic within 30 days after their last study drug dose for a treatment discontinuation visit.",
                "instanceType": "CommentAnnotation"
              },
              {
                "id": "soa_fn_5",
                "text": "[o] Serum pregnancy test within 7 days before Cycle 1, Day 1.",
                "instanceType": "CommentAnnotation"
              },
              {
                "id": "soa_fn_6",
                "text": "[p] Urine pregnancy test; if a urine pregnancy test is positive, it must be confirmed by a serum pregnancy test.",
                "instanceType": "CommentAnnotation"
              },
              {
                "id": "soa_fn_7",
                "text": "[q] TSH, free T3 and free T4 will be assessed within 96 hours before Day 1 of Cycle 1, and every second cycle thereafter.",
                "instanceType": "CommentAnnotation"
              },
              {
                "id": "soa_fn_8",
                "text": "[s] See Appendix 2 for a detailed schedule.",
                "instanceType": "CommentAnnotation"
              },
              {
                "id": "soa_fn_9",
                "text": "[w] Mandatory tumour tissue biopsy collected within 3 months prior to study enrolment.",
                "instanceType": "CommentAnnotation"
              },
              {
                "id": "soa_fn_10",
                "text": "[z] All patients will be followed for survival and new anti-cancer therapy information every 3 months ± 21 days.",
                "instanceType": "CommentAnnotation"
              }
            ],

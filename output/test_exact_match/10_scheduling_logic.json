      },
      {
        "id": "trans_3",
        "name": "Dose Interruption for Severe ALT Elevation",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If ALT > 5x ULN, temporarily interrupt dosing."
      },
      {

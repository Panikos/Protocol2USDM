    "conditions": [
      {
        "id": "cond_1",
        "name": "Dose Titration Safety Review",
        "instanceType": "Condition",
        "description": "Safety data must be reviewed by the Safety Review Committee (SRC) before dose titration.",
        "text": "Before titration to 30 mg/day, the Safety Review Committee (SRC) will review available safety data through Day 23 for each participant."
      },
      {
        "id": "cond_2",
        "name": "ALT/AST Elevation - Moderate",
        "instanceType": "Condition",
        "description": "Criteria for temporary dose interruption due to liver enzyme elevation.",
        "text": "ALT and/or AST > 3 to ≤ 5 × ULN"
      },
      {
        "id": "cond_3",
        "name": "ALT/AST Elevation - Severe",
        "instanceType": "Condition",
        "description": "Criteria for permanent dose discontinuation due to liver enzyme elevation.",
        "text": "ALT and/or AST > 5 × ULN"
      },
      {
        "id": "cond_4",
        "name": "Hemoglobin Decrease",
        "instanceType": "Condition",
        "description": "Criteria for dose reduction due to low hemoglobin.",
        "text": "Hemoglobin ≤ 10 g/dL"
      },
      {
        "id": "cond_5",
        "name": "Platelet Decrease",
        "instanceType": "Condition",
        "description": "Criteria for dose reduction due to low platelets.",
        "text": "Platelets ≤ 100,000/mm3"
      },
      {
        "id": "cond_6",
        "name": "Neutrophil Decrease",
        "instanceType": "Condition",
        "description": "Criteria for dose reduction due to low neutrophils.",
        "text": "Neutrophils > 30% decrease from baseline and below reference range at baseline"
      },
      {
        "id": "cond_7",
        "name": "Potential Hy's Law",
        "instanceType": "Condition",
        "description": "Criteria for temporary dose interruption due to concurrent bilirubin and ALT elevation.",
        "text": "Bilirubin > 2 × ULN accompanied by ALT > 3 × ULN"
      },
      {
        "id": "cond_8",
        "name": "Neurological Worsening",
        "instanceType": "Condition",
        "description": "Clinical finding that triggers evaluation for dose modification.",
        "text": "Evidence of neurologic worsening by AEs or by neurologic physical exam assessment"
      },
      {
        "id": "cond_9",
        "name": "Psychiatric Worsening",
        "instanceType": "Condition",
        "description": "Clinical finding that triggers evaluation for dose modification.",
        "text": "Evidence of clinically significant acute psychiatric worsening which may include but not limited to suicidality, acute depression, or psychosis."
      },
      {
        "id": "cond_10",
        "name": "Outpatient Period Extension",
        "instanceType": "Condition",
        "description": "Adaptive design feature allowing for extension of the outpatient period.",
        "text": "The Outpatient Period during Treatment Period 1 may be extended up to an additional 14 days with Investigator approval."
      }
    ],
    "conditionAssignments": [],
    "transitionRules": [
      {
        "id": "trans_1",
        "name": "Dose Titration to 30mg",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "fromElementId": "dose_15mg",
        "toElementId": "dose_30mg",
        "text": "Participants will be administered ALXN1840 at 15 mg/day for a treatment period of approximately 28 days followed by titration up to 30 mg/day on Day 29. This is conditional on SRC safety review."
      },
      {
        "id": "trans_2",
        "name": "Dose Interruption for Moderate LFT Elevation",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If ALT and/or AST > 3 to ≤ 5 × ULN, interrupt dosing. Rechallenge at previous dose level when ALT/AST is ≤ 2 × ULN."
      },
      {
        "id": "trans_3",
        "name": "Dose Discontinuation for Severe LFT Elevation",
        "instanceType": "TransitionRule",
        "transitionType": "Discontinuation",
        "text": "If ALT and/or AST > 5 × ULN, discontinue study intervention."
      },
      {
        "id": "trans_4",
        "name": "Dose Reduction for Anemia",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If Hemoglobin is ≤ 10 g/dL, reduce dose to previous dose level or to 15 mg QOD if on 15 mg QD."
      },
      {
        "id": "trans_5",
        "name": "Dose Reduction for Thrombocytopenia",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If Platelets are ≤ 100,000/mm3, reduce dose to previous dose level or to 15 mg QOD if on 15 mg QD."
      },
      {
        "id": "trans_6",
        "name": "Dose Reduction for Neutropenia",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If Neutrophils decrease > 30% from baseline and are below reference range, reduce dose to previous dose level or to 15 mg QOD if on 15 mg QD."
      },
      {
        "id": "trans_7",
        "name": "Dose Interruption for Potential Hy's Law",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If Bilirubin > 2 × ULN accompanied by ALT > 3 × ULN, temporarily interrupt dosing. Rechallenge requires approval of the Alexion Medical Monitor."
      },
      {
        "id": "trans_8",
        "name": "Dose Modification for Neurological Worsening",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "Investigator and Alexion Medical Monitor will evaluate the need for dose modification (interruption, increase or decrease) based on neurologic worsening."
      },
      {
        "id": "trans_9",
        "name": "Dose Modification for Psychiatric Worsening",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "Investigator and Alexion Medical Monitor will evaluate the need for dose modification (interruption, increase or decrease) based on psychiatric worsening."
      },
      {
        "id": "trans_10",
        "name": "Discontinuation of Intervention for Safety/Compliance",
        "instanceType": "TransitionRule",
        "transitionType": "Discontinuation",
        "text": "Participants must be considered for discontinuation from study intervention for: Serious hypersensitivity reaction; Severe uncontrolled infection; Use of disallowed medication; Pregnancy or planned pregnancy."
      }
    ],

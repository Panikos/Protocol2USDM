            "notes": [
              {
                "id": "soa_fn_1",
                "text": "Original informed consent in screening for protocol participation.",
                "instanceType": "CommentAnnotation"
              },
              {
                "id": "soa_fn_2",
                "text": "Assessed during screening period and (re-enrollment if applicable).",
                "instanceType": "CommentAnnotation"
              },
              {
                "id": "soa_fn_3",
                "text": "Within 14 days prior to first dose.",
                "instanceType": "CommentAnnotation"
              },
              {
                "id": "soa_fn_4",
                "text": "Height and Weight. Within 14 days prior to first dose.",
                "instanceType": "CommentAnnotation"
              },
              {
                "id": "soa_fn_5",
                "text": "Including BP, HR, temperature, respiratory rate. Obtain vital signs at the screening visit and within 72 hours prior to first dose.",
                "instanceType": "CommentAnnotation"
              },
              {
                "id": "soa_fn_6",
                "text": "2-D echocardiogram, performed locally, for valve and LVEF evaluation is to be done within 14 days prior to first dose and as clinically indicated.",
                "instanceType": "CommentAnnotation"
              },
              {
                "id": "soa_fn_7",
                "text": "Performed locally, within 14 days prior to randomization to include CBC w/differential and platelet count, LFTs (ALT, AST, total bilirubin, alkaline phosphatase), Alpha fetoprotein (AFP), BUN or serum urea level, albumin, creatinine, Ca+, Mg++, Na+, K+, Cl-, LDH, amylase, lipase, Glucose, TSH, Free T4, Free T3",
                "instanceType": "CommentAnnotation"
              },
              {
                "id": "soa_fn_8",
                "text": "Targeted examination must include at a minimum the following body systems: Cardiovascular, Gastrointestinal, Pulmonary, Skin",
                "instanceType": "CommentAnnotation"
              },
              {
                "id": "soa_fn_9",
                "text": "Serum or urine pregnancy test (minimum sensitivity 25 IU/L or equivalent units of HCG) to be done within 24 hours prior to first dose, and then monthly regardless of dosing schedule",
                "instanceType": "CommentAnnotation"
              }
            ],

              {
                "id": "act_1",
                "name": "Signed Informed Consent Form(s)",
                "instanceType": "Activity",
                "description": "Written informed consent can be obtained up to 30 days prior to study entry."
              },
              {
                "id": "act_2",
                "name": "Review of eligibility criteria",
                "instanceType": "Activity",
                "description": "Review of eligibility criteria"
              },
              {
                "id": "act_3",
                "name": "Medical, surgical, and cancer histories, including demographic information",
                "instanceType": "Activity",
                "description": "Cancer history includes stage, date of diagnosis, and prior anti-tumor treatment. Demographic information includes age and self-reported race/ethnicity."
              },
              {
                "id": "act_4",
                "name": "Complete physical examination",
                "instanceType": "Activity",
                "description": "Evaluation of head, eye, ear, nose, and throat and cardiovascular, dermatologic, musculoskeletal, respiratory, gastrointestinal, genitourinary, and neurologic systems."
              },
              {
                "id": "act_5",
                "name": "Limited physical examination",
                "instanceType": "Activity",
                "description": "Performed at other visits to assess changes from baseline abnormalities and any new abnormalities."
              },
              {
                "id": "act_6",
                "name": "ECOG Performance Status",
                "instanceType": "Activity",
                "description": "ECOG Performance Status"
              },
              {
                "id": "act_7",
                "name": "Patient-reported outcomes",
                "instanceType": "Activity",
                "description": "EORTC QLQ-C30, EORTC QLQ-HCC18, and EQ-5D-5L questionnaires."
              },
              {
                "id": "act_8",
                "name": "Tumor assessment",
                "instanceType": "Activity",
                "description": "Radiologic imaging (CT/MRI) performed every 6 weeks for the first 54 weeks."
              },
              {
                "id": "act_9",
                "name": "Vital signs",
                "instanceType": "Activity",
                "description": "Heart rate, respiratory rate, blood pressure, and temperature."
              },
              {
                "id": "act_10",
                "name": "Weight",
                "instanceType": "Activity",
                "description": "Weight in kilograms."
              },
              {
                "id": "act_11",
                "name": "Height",
                "instanceType": "Activity",
                "description": "Height"
              },
              {
                "id": "act_12",
                "name": "12-lead ECG",
                "instanceType": "Activity",
                "description": "Patients should be resting and in a supine position for at least 10 minutes."
              },
              {
                "id": "act_13",
                "name": "EGD",
                "instanceType": "Activity",
                "description": "Esophagogastroduodenscopy to assess varices."
              },
              {
                "id": "act_14",
                "name": "Hematology",
                "instanceType": "Activity",
                "description": "CBC including RBC, hemoglobin, hematocrit, WBC with differential, and platelet count."
              },
              {
                "id": "act_15",
                "name": "Serum chemistry",
                "instanceType": "Activity",
                "description": "Includes electrolytes, glucose, BUN, creatinine, liver function tests, and LDH."
              },
              {
                "id": "act_16",
                "name": "HIV, HBV, HCV serology",
                "instanceType": "Activity",
                "description": "HBsAg, HBcAb, and HBsAb collected during screening."
              },
              {
                "id": "act_17",
                "name": "Quantitative HBsAg, HBV DNA, HCV RNA",
                "instanceType": "Activity",
                "description": "Tested if patient is positive for HBsAg, HBcAb, or HCV antibody."
              },
              {
                "id": "act_18",
                "name": "α-fetoprotein",
                "instanceType": "Activity",
                "description": "α-fetoprotein"
              },
              {
                "id": "act_19",
                "name": "Coagulation panel (aPTT, INR)",
                "instanceType": "Activity",
                "description": "Coagulation panel"
              },
              {
                "id": "act_20",
                "name": "Urinalysis",
                "instanceType": "Activity",
                "description": "Urine dipstick including specific gravity, pH, glucose, protein, ketones, and blood."
              },
              {
                "id": "act_21",
                "name": "TSH, free T3, free T4",
                "instanceType": "Activity",
                "description": "Thyroid function tests every 4 cycles."
              },
              {
                "id": "act_22",
                "name": "Pregnancy test",
                "instanceType": "Activity",
                "description": "Serum or urine pregnancy test for women of childbearing potential."
              },
              {
                "id": "act_23",
                "name": "Serum PK sample",
                "instanceType": "Activity",
                "description": "Atezolizumab PK samples."
              },
              {
                "id": "act_24",
                "name": "Serum ADA sample",
                "instanceType": "Activity",
                "description": "Atezolizumab ADA samples."
              },
              {
                "id": "act_25",
                "name": "Pharmacodynamic samples for biomarkers",
                "instanceType": "Activity",
                "description": "Biomarker samples (plasma, serum)."
              },
              {
                "id": "act_26",
                "name": "Plasma sample for RBR (optional)",
                "instanceType": "Activity",
                "description": "Research Biosample Repository sample."
              },
              {
                "id": "act_27",
                "name": "Archival tumor tissue sample",
                "instanceType": "Activity",
                "description": "Archival tumor tissue sample or optional fresh biopsy."
              },
              {
                "id": "act_28",
                "name": "Concomitant medications",
                "instanceType": "Activity",
                "description": "Any prescription or over-the-counter medications."
              },
              {
                "id": "act_29",
                "name": "Adverse events",
                "instanceType": "Activity",
                "description": "Reporting of all adverse events and serious adverse events."
              },
              {
                "id": "act_30",
                "name": "Study treatment infusion",
                "instanceType": "Activity",
                "description": "Atezolizumab and bevacizumab infusions."
              },
              {
                "id": "act_31",
                "name": "Sorafenib dispensing",
                "instanceType": "Activity",
                "description": "Sorafenib taken by mouth twice a day continuously."
              },
              {
                "id": "act_32",
                "name": "Survival and anti-cancer therapy follow-up",
                "instanceType": "Activity",
                "description": "Collected via telephone calls, medical records, and/or clinic visits every 3 months."
              }

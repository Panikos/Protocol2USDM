      },
      {
        "id": "trans_3",
        "name": "Dose Interruption for ALT/AST Level 2",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If ALT/AST > 5x to <= 8x ULN, temporarily interrupt dosing."
      },
      {
        "id": "trans_4",
        "name": "Discontinuation for ALT/AST Level 3",
        "instanceType": "TransitionRule",
        "transitionType": "Discontinuation",
        "text": "If ALT/AST > 8x ULN, discontinue study intervention."
      },
      {
        "id": "trans_5",
        "name": "Dose Reduction for Anemia",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If Hemoglobin < 10 g/dL, reduce dose to previous dose level or 15 mg QOD if on 15 mg QD."
      },
      {
        "id": "trans_6",
        "name": "Dose Reduction for Thrombocytopenia",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If Platelets < 100,000/mm3, reduce dose to previous dose level or 15 mg QOD if on 15 mg QD."
      },
      {
        "id": "trans_7",
        "name": "Dose Reduction for Neutropenia",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If Neutrophils > 30% decrease from baseline and below reference range, reduce dose to previous dose level or 15 mg QOD if on 15 mg QD."
      },
      {
        "id": "trans_8",
        "name": "Dose Interruption for Liver Injury Signal",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If Bilirubin > 2x ULN and ALT > 3x ULN, temporarily interrupt dosing."
      },

    "transitionRules": [
      {
        "id": "trans_1",
        "name": "Dose Titration to 30mg",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "Participants will be administered ALXN1840 at 15 mg/day for a treatment period of approximately 28 days followed by titration up to 30 mg/day on Day 29."
      },
      {
        "id": "trans_2",
        "name": "Dose Reduction for ALT/AST",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If ALT and/or AST > 3 to ≤ 5 × ULN, reduce dose to previous dose level or 15 mg QOD if on 15 mg QD."
      },
      {
        "id": "trans_3",
        "name": "Dose Interruption for ALT/AST",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If ALT and/or AST > 5 to ≤ 8 × ULN, temporarily interrupt dosing."
      },
      {
        "id": "trans_4",
        "name": "Discontinuation for ALT/AST",
        "instanceType": "TransitionRule",
        "transitionType": "Discontinuation",
        "text": "If ALT and/or AST > 8 × ULN, discontinue study intervention."
      },
      {
        "id": "trans_5",
        "name": "Dose Reduction for Hemoglobin",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If Hemoglobin > 2 g/dL ↓ from baseline, reduce dose to previous dose level or 15 mg QOD if on 15 mg QD."
      },
      {
        "id": "trans_6",
        "name": "Dose Interruption for Hemoglobin",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If Hemoglobin > 3 g/dL ↓ from baseline, temporarily interrupt dosing."
      },
      {
        "id": "trans_7",
        "name": "Discontinuation for Hypersensitivity",
        "instanceType": "TransitionRule",
        "transitionType": "Discontinuation",
        "text": "Participants must be considered for discontinuation from study intervention if a serious hypersensitivity reaction occurs."
      },
      {
        "id": "trans_8",
        "name": "Discontinuation for Infection",
        "instanceType": "TransitionRule",
        "transitionType": "Discontinuation",
        "text": "Participants must be considered for discontinuation from study intervention for a severe uncontrolled infection."
      },
      {
        "id": "trans_9",
        "name": "Discontinuation for Disallowed Medication",
        "instanceType": "TransitionRule",
        "transitionType": "Discontinuation",
        "text": "Participants must be considered for discontinuation from study intervention for use of disallowed medication."
      },
      {
        "id": "trans_10",
        "name": "Discontinuation for Pregnancy",
        "instanceType": "TransitionRule",
        "transitionType": "Discontinuation",
        "text": "Participants must be considered for discontinuation from study intervention for pregnancy or planned pregnancy."
      }

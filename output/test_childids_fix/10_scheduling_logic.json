      },
      {
        "id": "cond_3",
        "name": "Dose Rechallenge Limit",
        "instanceType": "Condition",
        "description": "A maximum number of dose rechallenges is permitted following a dose modification.",
        "text": "A maximum of 3 rechallenges will be allowed."
      },
      {
        "id": "cond_4",
        "name": "Dosing Fasting and Meal Condition",
        "instanceType": "Condition",
        "description": "Conditions related to fasting and meal timing around study drug administration.",
        "text": "Study intervention will be administered after an overnight fast (ie, at least 10 hours)... meals should be delayed a minimum of 2 hours after dosing."
      },
      {
        "id": "cond_5",
        "name": "ALT/AST Elevation - Level 1",
        "instanceType": "Condition",
        "description": "Criteria for dose reduction due to moderate transaminase elevation.",
        "text": "ALT and/or AST > 3 to ≤ 5 × ULN"
      },
      {
        "id": "cond_6",
        "name": "ALT/AST Elevation - Level 2",
        "instanceType": "Condition",
        "description": "Criteria for dose interruption due to significant transaminase elevation.",
        "text": "ALT and/or AST > 5 to ≤ 8 × ULN"
      },
      {
        "id": "cond_7",
        "name": "ALT/AST Elevation - Level 3",
        "instanceType": "Condition",
        "description": "Criteria for study intervention discontinuation due to severe transaminase elevation.",
        "text": "ALT and/or AST > 8 × ULN"
      },
      {
        "id": "cond_8",
        "name": "Hemoglobin Decrease - Level 1",
        "instanceType": "Condition",
        "description": "Criteria for dose reduction due to moderate hemoglobin decrease.",
        "text": "Hemoglobin > 2 g/dL ↓ from baseline"
      },
      {
        "id": "cond_9",
        "name": "Hemoglobin Decrease - Level 2",
        "instanceType": "Condition",
        "description": "Criteria for dose interruption due to significant hemoglobin decrease.",
        "text": "Hemoglobin > 3 g/dL ↓ from baseline"
      },
      {
        "id": "cond_10",
        "name": "Platelet Decrease",
        "instanceType": "Condition",
        "description": "Criteria for dose reduction due to platelet decrease.",
        "text": "Platelets > 30% ↓ from baseline"
      },
      {
        "id": "cond_11",
        "name": "Neutrophil Decrease",
        "instanceType": "Condition",
        "description": "Criteria for dose reduction due to neutrophil decrease.",
        "text": "Neutrophils > 30% ↓ from baseline"
      },
      {
        "id": "cond_12",
        "name": "Hy's Law Condition",
        "instanceType": "Condition",
        "description": "Criteria for dose interruption due to concurrent bilirubin and ALT elevation.",
        "text": "Bilirubin > 2 × ULN Accompanied by ALT > 3 × ULN"
      },
      {
        "id": "cond_13",
        "name": "Neurologic Worsening",
        "instanceType": "Condition",
        "description": "Criteria for evaluating dose modification due to neurologic worsening.",
        "text": "Evidence of neurologic worsening by AEs or by neurologic physical exam assessment"
      }
    ],

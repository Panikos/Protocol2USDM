    "conditions": [
      {
        "id": "cond_1",
        "name": "ALT/AST Elevation for Dose Interruption",
        "instanceType": "Condition",
        "description": "Criteria for temporarily interrupting ALXN1840 dosing due to elevated liver transaminases.",
        "text": "ALT and/or AST > 3 × ULN and ≤ 5 × ULN"
      },
      {
        "id": "cond_2",
        "name": "ALT/AST Elevation for Discontinuation",
        "instanceType": "Condition",
        "description": "Criteria for permanently discontinuing ALXN1840 dosing due to elevated liver transaminases.",
        "text": "ALT and/or AST > 5 × ULN"
      },
      {
        "id": "cond_3",
        "name": "Neutropenia for Dose Interruption",
        "instanceType": "Condition",
        "description": "Criteria for temporarily interrupting ALXN1840 dosing due to low neutrophil count.",
        "text": "Neutrophils < 1.0 × 10^9/L"
      },
      {
        "id": "cond_4",
        "name": "Neutrophil Decrease for Dose Reduction",
        "instanceType": "Condition",
        "description": "Criteria for reducing ALXN1840 dose due to a significant decrease in neutrophil count.",
        "text": "> 30% decrease from baseline and Neutrophils below reference range at baseline"
      },
      {
        "id": "cond_5",
        "name": "Potential Liver Injury (Hy's Law)",
        "instanceType": "Condition",
        "description": "Criteria for temporarily interrupting ALXN1840 dosing due to concurrent elevation of bilirubin and ALT.",
        "text": "Bilirubin > 2 × ULN Accompanied by ALT > 3 × ULN, indicative of liver injury"
      },
      {
        "id": "cond_6",
        "name": "Neurologic Worsening",
        "instanceType": "Condition",
        "description": "Clinical evidence of neurologic worsening that triggers an evaluation for dose modification.",
        "text": "Evidence of neurologic worsening by AEs or by neurologic physical exam assessment"
      },
      {
        "id": "cond_7",
        "name": "Psychiatric Worsening",
        "instanceType": "Condition",
        "description": "Clinical evidence of psychiatric worsening that triggers an evaluation for dose modification.",
        "text": "Evidence of clinically significant acute psychiatric worsening which may include but not limited to suicidality, acute depression, or psychosis."
      },
      {
        "id": "cond_8",
        "name": "Safety Review for Dose Titration",
        "instanceType": "Condition",
        "description": "The Safety Review Committee must review safety data before a participant can be titrated to a higher dose.",
        "text": "Before titration to 30 mg/day, the Safety Review Committee (SRC) will review available safety data through Day 23 for each participant."
      },
      {
        "id": "cond_9",
        "name": "Renal Function Eligibility",
        "instanceType": "Condition",
        "description": "Exclusion criterion based on creatinine clearance.",
        "text": "creatinine clearance < 30 mL/min"
      }

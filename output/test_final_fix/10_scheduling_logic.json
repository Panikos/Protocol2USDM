    "transitionRules": [
      {
        "id": "trans_1",
        "name": "Titration to 30mg/day",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "On Day 29, participants will be administered ALXN1840 at 15 mg/day and then increase to 30 mg/day, conditional on SRC safety review through Day 23."
      },
      {
        "id": "trans_2",
        "name": "Dose Reduction for Moderate ALT Elevation",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If ALT > 3x ULN and <= 5x ULN, reduce dose to previous dose level or to 15 mg QOD if on 15 mg QD."
      },
      {
        "id": "trans_3",
        "name": "Dose Interruption for Severe ALT Elevation",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If ALT > 5x ULN, temporarily interrupt dosing."
      },
      {
        "id": "trans_4",
        "name": "Dose Reduction for Moderate Thrombocytopenia",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If Platelets < 100,000/mm3, reduce dose to previous dose level or to 15 mg QOD if on 15 mg QD."
      },
      {
        "id": "trans_5",
        "name": "Dose Interruption for Severe Thrombocytopenia",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If Platelets < 75,000/mm3, temporarily interrupt dosing."
      },
      {
        "id": "trans_6",
        "name": "Dose Reduction for Moderate Anemia",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If Hemoglobin < 10 g/dL, reduce dose to previous dose level or to 15 mg QOD if on 15 mg QD."
      },
      {
        "id": "trans_7",
        "name": "Dose Interruption for Severe Anemia",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If Hemoglobin < 8 g/dL, temporarily interrupt dosing."
      },
      {
        "id": "trans_8",
        "name": "Dose Reduction for Neutropenia",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If Neutrophils > 30% decrease from baseline and below reference range, reduce dose to previous dose level or to 15 mg QOD if on 15 mg QD."
      },
      {
        "id": "trans_9",
        "name": "Dose Interruption for Potential Hy's Law",
        "instanceType": "TransitionRule",
        "transitionType": "Dose Modification",
        "text": "If Bilirubin > 2x ULN and ALT > 3x ULN, temporarily interrupt dosing."
      },
      {
        "id": "trans_10",
        "name": "Discontinuation for Safety Events",
        "instanceType": "TransitionRule",
        "transitionType": "Discontinuation",
        "text": "Participants must be considered for discontinuation from study intervention for: Serious hypersensitivity reaction; Severe uncontrolled infection; Use of disallowed medication; Pregnancy or planned pregnancy."
      }
